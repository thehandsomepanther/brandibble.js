{
  parser: "babel-eslint",
  "extends": "airbnb",
  "rules": {
    'arrow-body-style': 0,
    'camelcase': 0,
    'consistent-return': 0,
    'import/extensions': 0,
    'import/no-extraneous-dependencies': 0,
    'import/no-unresolved': 0,
    'no-bitwise': 0,
    'no-extend-native': 0,
    'no-mixed-operators': 0,
    'no-return-assign': 0,
    'no-underscore-dangle': 0,
    'no-unused-expressions': 0,
    'max-len': 0,
    'radix': 0,
  }
}
